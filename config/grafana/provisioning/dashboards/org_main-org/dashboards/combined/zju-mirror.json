          "pluginVersion": "4.8.2",
          "queryType": "table",
          "rawSql": "SELECT toStartOfInterval(\n    Timestamp,\n    -- 根据 grafana 的查询时间范围调整聚合粒度\n    toIntervalMillisecond($__interval_ms * 30)\n  ) as timestamp,\n  replaceRegexpOne(\n    -- 从 URL 中提取仓库名\n    LogAttributes ['url.path'],\n    '^/([^/]+)/.*',\n    '\\\\1'\n  ) AS repo,\n  COUNT(*)\nFROM default.otel_logs\nWHERE (\n    Timestamp \u003e= $__fromTime\n    AND Timestamp \u003c= $__toTime\n  )\n  AND (\n    ResourceAttributes ['cloud.region'] = 'zjusct-falcon'\n  )\n  AND (ResourceAttributes ['host.name'] = 'zjumirror')\n  AND (LogAttributes ['log.file.name'] = 'otel.log')\n  AND (\n    match(LogAttributes ['url.path'], '^/([^/]+)/.*')\n  )\nGROUP BY ALL -- 按请求次数降序排列，然后对每个时间段取前 5 个\nORDER BY COUNT(*) DESC\nLIMIT 5 BY timestamp;\n",
          "refId": "A"
        }
      ],
      "title": "Most used repos",
      "transformations": [
        {
          "id": "prepareTimeSeries",
          "options": {
            "format": "multi"
          }
        },
        {
          "id": "labelsToFields",
          "options": {
            "mode": "columns",
            "valueLabel": "repo"
          }
        },
        {
          "id": "renameByRegex",
          "options": {
            "regex": "(.*)",
            "renamePattern": "$1"
          }
        }
      ],
      "type": "heatmap"
